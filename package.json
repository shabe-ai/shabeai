{
  "name": "shabeai-monorepo",
  "private": true,
  "version": "0.0.0",
  "workspaces": [
    "frontend",
    "backend"
  ],
  "scripts": {
    "dev": "pnpm --filter frontend dev",
    "build": "pnpm --filter frontend build",
    "lint": "pnpm --filter frontend lint",
    "test": "pnpm --filter shabeai-backend test",
    "generate-openapi": "pnpm --filter shabeai-backend openapi-json && pnpm --filter frontend update-api"
  },
  "dependencies": {
    "@tanstack/react-query": "5.80.6",
    "@tanstack/react-query-devtools": "^5.80.6",
    "axios": "^1.9.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "js-cookie": "^3.0.5",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "stripe": "^18.3.0"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/postcss": "^4.1.8",
    "@types/js-cookie": "^3.0.6",
    "@types/react-query": "^1.2.9",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.4",
    "tailwindcss": "^4.1.8"
  },
  "pnpm": {
    "overrides": {
      "@tanstack/react-query": "5.80.6",
      "@tanstack/query-core": "5.80.6",
      "tailwindcss-merge": "npm:tailwind-merge@^2.3.0"
    },
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "@tailwindcss/oxide",
      "esbuild"
    ]
  }
}